# Phone Agent Configuration
# Copy this file to .env and fill in your values

# Model API endpoint
# - 智谱 API: https://open.bigmodel.cn/api/paas/v4
# - 本地部署: http://localhost:8000/v1
PHONE_AGENT_BASE_URL=https://open.bigmodel.cn/api/paas/v4

# Model name
PHONE_AGENT_MODEL=autoglm-phone

# API Key (从智谱开放平台获取: https://open.bigmodel.cn)
PHONE_AGENT_API_KEY=your-api-key-here

# Maximum steps per task (optional, default: 100)
# PHONE_AGENT_MAX_STEPS=100

# ADB device ID (optional, for multi-device setups)
# PHONE_AGENT_DEVICE_ID=
